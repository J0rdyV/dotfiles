#!/bin/sh

redshift -O 5000k &   # Remove blue light
clipmenud &           # Enable clipboard history
xcompmgr &            # Enable opacity
numlockx &            # Set numlock
dunst &               # Notifications
dwmtime-batt &
dwmtime-news &
dwmtime &             # Put time in head
fixscreen &           # Display tweaks
trash-empty -f 30 &

# fix webcam
v4l2-ctl -d /dev/video0 -c saturation=68 &
v4l2-ctl -d /dev/video0 -c brightness=130 &
v4l2-ctl -d /dev/video0 -c contrast=30 &

# fix gnome-keyring
dbus-update-activation-environment --all

ssh-agent dwm         # Start WM
#ssh-agent rio -term st
